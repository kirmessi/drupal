/**
 * Implements hook_theme_suggestions_HOOK_alter().
 */
function laptops_phones_theme_suggestions_region_alter(&$suggestions, $vars, $hook) {
  //Highlighting regions on the page
  foreach ($suggestions as $suggestion) {
    echo '<h2 style="font-weight:700">** ' . $suggestion . ' **</h2>';
  }
}
